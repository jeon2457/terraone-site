{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],

  "rewrites": [
    { "source": "/admin_firebase.html", "destination": "/admin_firebase.html" },
    { "source": "**", "destination": "/index.html" }
  ]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "database": {
      "port": 9000
    },
    "ui": {
      "enabled": true
    }
  }
}